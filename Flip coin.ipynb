    "import random\n",
    "heads_count = 0\n",
    "tails_count = 0\n",
    "for _ in range(10):\n",
    "    flip = random.choice([\"Heads\",\"tails\"])\n",
    "    if flip == \"Heads\":\n",
    "        heads_count+=1\n",
    "    else:\n",
    "        tails_count+=1\n",
    "print(\"Heads Count\" , heads_count)\n",
    "print(\"Tails Count\", tails_count)"
   ]
  }
